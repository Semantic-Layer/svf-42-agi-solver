{
	"name": "svf-agi-solver-internal",
	"version": "1.0.0",
	"description": "svf agi solver",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Semantic-Layer/svf-agi-solver-internal.git"
	},
	"license": "MIT",
	"type": "module",
	"main": "dist/index.js",
	"files": [
		"LICENSE.md",
		"README.md",
		"dist/",
		"package.json"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsx watch src/index.ts",
		"fmt": "prettier --write .",
		"fmt:check": "prettier --check .",
		"fmt:src": "prettier --write \"src/**/*.{ts,tsx}\"",
		"format": "prettier .",
		"preinstall": "npx only-allow pnpm",
		"lint": "eslint . --max-warnings 0",
		"prepare": "husky",
		"start": "node dist/index.js",
		"start:dev": "tsx src/index.ts",
		"test": "echo \"Error: no test specified\" && exit 1",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"dependencies": {
		"@lifi/sdk": "^3.6.5",
		"dotenv": "^16.4.7",
		"viem": "^2.23.13",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"@eslint/js": "9.22.0",
		"@types/node": "22.13.10",
		"eslint": "9.22.0",
		"husky": "9.1.7",
		"lint-staged": "15.5.0",
		"prettier": "3.5.3",
		"ts-node": "^10.9.2",
		"tsup": "8.4.0",
		"tsx": "^4.19.3",
		"typescript": "5.8.2",
		"typescript-eslint": "8.26.1"
	},
	"packageManager": "pnpm@10.7.1",
	"engines": {
		"node": ">=23"
	}
}
